@inproceedings{freitas_asmes_2003-1,
	address = {Reno, Nevada},
	title = {{ASME}'s quest to quantify numerical uncertainty},
	abstract = {Methods for the quantification of numerical
uncertainty have been a subject of interest to
ASME and the mechanical engineering community
as a whole for over a decade. During this time
period, ASME has promulgated two statements of
standards for the reporting of numerical uncertainty
in archival publications. This paper reviews the
work that has gone into the specification of these
standards, and the formulation of methods for
quantifying numerical uncertainty. The objective
of this paper is not to define a strategy for
verification and validation as procedures, but rather
to focus on the estimation of the magnitude of
numerical uncertainty in a given simulation.
Further, this paper makes no judgment on what is
an acceptable level of uncertainty in a simulation.
The premise behind this paper is simply the
realization that methods for predicting the
magnitude of uncertainty or for banding the
magnitude of the error are required for any
verification and validation procedure. Clearly, any
of a number of possible methods may be used to
calculate numerical uncertainty. The present paper
presents one possible approach. This approach is a
five-step procedure for the estimation of numerical
uncertainty based on Richardson Extrapolation and
measured by the Grid Convergence Index.},
	language = {en},
	booktitle = {{AIAA} 2003-627},
	author = {Freitas, C. J. and Ghia, U and Celik, I. and Roache, P. and Raad, P.},
	month = jan,
	year = {2003},

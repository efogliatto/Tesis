}

@article{vasilyev_high_2000,
	title = {High {Order} {Finite} {Difference} {Schemes} on {Non}-uniform {Meshes} with {Good} {Conservation} {Properties}},
	volume = {157},
	issn = {0021-9991},
	url = {http://www.sciencedirect.com/science/article/pii/S0021999199963982},
	doi = {10.1006/jcph.1999.6398},
	abstract = {Numerical simulation of turbulent flows (DNS or LES) requires numerical methods that are both stable and free of numerical dissipation. One way to achieve this is to enforce additional constraints, such as discrete conservation of mass, momentum, and kinetic energy. The objective of this work is to generalize the high order schemes of Morinishi et al. to non-uniform meshes while maintaining conservation properties of the schemes as much as possible. This generalization is achieved by preserving symmetries of the uniform mesh case. The proposed schemes do not simultaneously conserve mass, momentum, and kinetic energy. However, depending on the form of the convective term, conservation of either momentum or energy in addition to mass can be achieved. It is shown that the conservation properties of the generalized schemes are as good as those of the standard second order finite difference scheme on non-uniform meshes, while the accuracy of the new schemes is definitely superior. The predicted conservation properties are demonstrated numerically in inviscid flow simulations.},
	number = {2},
	urldate = {2013-09-26},
	journal = {Journal of Computational Physics},
	author = {Vasilyev, Oleg V.},
	month = jan,
	year = {2000},
	pages = {746--761},
	file = {Vasilyev - 2000 - High Order Finite Difference Schemes on Non-unifor.pdf:/home/fogliate/Zotero/storage/48E8HD2C/Vasilyev - 2000 - High Order Finite Difference Schemes on Non-unifor.pdf:application/pdf;ScienceDirect Snapshot:/home/fogliate/Zotero/storage/8EAH2JDQ/S0021999199963982.html:text/html}
}

@article{singh_multi-block_2011,
	title = {A multi-block {ADI} finite-volume method for incompressible {Navier}{\textendash}{Stokes} equations in complex geometries},
	volume = {230},
	issn = {0021-9991},
	url = {http://www.sciencedirect.com/science/article/pii/S002199911100355X},
	doi = {10.1016/j.jcp.2011.06.006},
	abstract = {An efficient second-order accurate finite-volume method is developed for a solution of the incompressible Navier{\textendash}Stokes equations on complex multi-block structured curvilinear grids. Unlike in the finite-volume or finite-difference-based alternating-direction-implicit (ADI) methods, where factorization of the coordinate transformed governing equations is performed along generalized coordinate directions, in the proposed method, the discretized Cartesian form Navier{\textendash}Stokes equations are factored along curvilinear grid lines. The new ADI finite-volume method is also extended for simulations on multi-block structured curvilinear grids with which complex geometries can be efficiently resolved. The numerical method is first developed for an unsteady convection{\textendash}diffusion equation, then is extended for the incompressible Navier{\textendash}Stokes equations. The order of accuracy and stability characteristics of the present method are analyzed in simulations of an unsteady convection{\textendash}diffusion problem, decaying vortices, flow in a lid-driven cavity, flow over a circular cylinder, and turbulent flow through a planar channel. Numerical solutions predicted by the proposed ADI finite-volume method are found to be in good agreement with experimental and other numerical data, while the solutions are obtained at much lower computational cost than those required by other iterative methods without factorization. For a simulation on a grid with O(105) cells, the computational time required by the present ADI-based method for a solution of momentum equations is found to be less than 20\% of that required by a method employing a biconjugate-gradient-stabilized scheme.},
	number = {19},
	urldate = {2013-09-20},
	journal = {Journal of Computational Physics},
	author = {Singh, Satbir and You, Donghyun},
	month = aug,
	year = {2011},
	keywords = {Alternating-direction-implicit (ADI) method, Complex geometry, Finite-volume method, Multi-block grid, Navier{\textendash}Stokes equations},
	pages = {7400--7417},
	file = {Singh and You - 2011 - A multi-block ADI finite-volume method for incompr.pdf:/home/fogliate/Zotero/storage/8FC5ZM8Q/Singh and You - 2011 - A multi-block ADI finite-volume method for incompr.pdf:application/pdf;ScienceDirect Snapshot:/home/fogliate/Zotero/storage/2IBFVJ4R/S002199911100355X.html:text/html}
}

@article{morinishi_fully_2004,
	title = {Fully conservative finite difference scheme in cylindrical coordinates for incompressible flow simulations},

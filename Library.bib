	file = {Zhang et al. - 2020 - The effects of wall superheat and surface wettabil.pdf:/home/fogliate/Zotero/storage/7RKMWEXB/Zhang et al. - 2020 - The effects of wall superheat and surface wettabil.pdf:application/pdf}
}

@article{liu_experimental_2019,
